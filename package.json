    "jquery": "^3.3.1",
    "koa-ejs": "^4.1.1",
    "mobx": "^4.2.0",
    "mobx-react": "^5.1.2",
    "mobx-react-router": "^4.0.3",
    "moment": "^2.22.1",
    "normalize.css": "^8.0.0",
    "ramda": "^0.25.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-helmet": "^5.2.0",
    "react-loadable": "^5.4.0",
    "react-router-dom": "^4.2.2",
    "typescript": "^2.8.3"
  },
